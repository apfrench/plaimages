<p id="notice"><%= notice %></p>

<h1>Listing Plaimages</h1>

<table class="table table-hover table-condensed">
  <thead>
    <tr>
      <th>Latitude</th>
      <th>Longitude</th>
      <th>Description</th>
      <th>Comment</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @plaimages.each do |plaimage| %>
      <tr>
        <td><%= plaimage.latitude %></td>
        <td><%= plaimage.longitude %></td>
        <td><%= plaimage.description %></td>
        <td><%= plaimage.comment %></td>
        <td><%= link_to 'Show', plaimage, class: "btn btn-link" %></td>
        <td><%= link_to 'Edit', edit_plaimage_path(plaimage), class: "btn btn-link" %></td>
        <td><%= link_to 'Destroy', plaimage, method: :delete, data: { confirm: 'Are you sure?' }, class: "btn btn-link" %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Plaimage', new_plaimage_path %>
